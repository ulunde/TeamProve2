<div class='container'>
    <h1><%= user.name %> </h1>
    <hr>
    <h3>Privilege Level:</h3>
    <% if (user.admin) { %>
    <img src="/images/coach.png">Coach
    <% } else { %>
    <img src="/images/player.png">Player
    <% } %>

    <h4>Squad: <%= user.title %> </h4>

    <h4>Email: <%= user.email %> </h4>
    <h4>Telephone Number: <%= user.phone %> </h4>
    <h4>Age: <%= user.age %> </h4>


    <a class="btn btn-medium btn-primary" href="/user/edit/<%= user.id %>">Edit User</a>
    <a class="btn btn-medium btn-success" href="/user/exercise/<%= user.id %>">Add Exercise</a>
    <a class="btn btn-medium btn-info" href="/user/viewexercise/<%= user.id %>">view Exercise</a>


</div>